<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/dashboardActivity">

    <activity
        android:id="@+id/dashboardActivity"
        android:name="com.bylancer.classified.bylancerclassified.dashboard.DashboardActivity"
        android:label="DashboardActivity" />

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.bylancer.classified.bylancerclassified.dashboard.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard">
        <action
            android:id="@+id/action_dashboardFragment_to_searchCategaoryFragment"
            app:destination="@id/searchCategaoryFragment" />
    </fragment>

    <fragment
        android:id="@+id/searchCategaoryFragment"
        android:name="com.bylancer.classified.bylancerclassified.search.SearchCategaoryFragment"
        android:label="fragment_search_categaory"
        tools:layout="@layout/fragment_search_categaory" />

</navigation>